#: GlobalPlugins/umStart.py:19
msgid "Abre unigram, o lo enfoca si ya se encuentra abierto"
msgstr "Abre o unigram, ou coloca-o sob o foco, se já estiver aberto"

#: GlobalPlugins/umStart.py:27
msgid "No se encuentra la aplicación"
msgstr "Aplicação não encontrada"

#: appModules/unigram.py:25
msgid "Solo disponible desde la lista de mensajes"
msgstr "Apenas disponível na lista de mensagens"

#: appModules/unigram.py:50
msgid "Pulsa el botón compartir"
msgstr "pressionar o botão compartilhar"

#: appModules/unigram.py:63
msgid "Enfoca la lista de chats"
msgstr "Leva o foco para a lista de conversas"

#: appModules/unigram.py:76
msgid "Enfoca la lista de mensajes del chat abierto"
msgstr "Leva o foco para a lista de mensagens da conversa aberta"

#: appModules/unigram.py:85
msgid "No se ha encontrado ningún chat abierto"
msgstr "Não foi encontrada nenhuma conversa aberta"

#: appModules/unigram.py:90
msgid "Enfoca los mensajes no leídos del chat abierto"
msgstr "Leva o foco para as mensagens não lidas da conversa aberta"

#: appModules/unigram.py:102
msgid "No se han encontrado mensajes no leídos"
msgstr "Não foram encontradas mensagens não lidas"

#: appModules/unigram.py:107
msgid "Descarga el archivo adjunto"
msgstr "Descarregar o ficheiro anexo"

#: appModules/unigram.py:119
msgid "Activa y desactiva la velocidad doble de un mensaje de voz"
msgstr "Liga e desliga a dupla velocidade de uma mensagem de voz"

#: appModules/unigram.py:130
msgid "Velocidad doble"
msgstr "Dupla velocidade"

#: appModules/unigram.py:132
msgid "velocidad normal"
msgstr "velocidade normal"

#: appModules/unigram.py:140
msgid "Pulsa el botón adjuntar"
msgstr "pressionar o botão anexar"

#: appModules/unigram.py:163
msgid "Verbaliza el nombre y estado del chat actual"
msgstr "Verbaliza o nome e o estado da conversa actual"

#: appModules/unigram.py:178
msgid "Pulsa el botón llamada de audio"
msgstr "pressionar o botão de chamada de áudio"

#: appModules/unigram.py:196
msgid "Pulsa el botón llamada de video"
msgstr "pressionar o botão de videochamada"
